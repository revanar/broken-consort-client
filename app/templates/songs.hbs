<div>
    <h2>Transcribed Renaissance Music</h2>
    <table>
        <tr>
            <th>Song</th>
            <th>Song #</th>
            <th>Book</th>
            <th>Editor</th>
            <th>Year</th>
            <th>Parts #</th>
            <th>Has text?</th>
        </tr>
        {{#each model as |song|}}

            <tr class="listing">
                <td>{{song.name}}</td>
                <td>{{song.song_no}}</td>
                <td>{{song.book.name}}</td>
                <td>{{song.book.editor.name}}</td>
                <td>{{song.book.year}}</td>
                <td>{{song.parts_no}}</td>
                <td>{{song.has_text}}</td>
                <td><a href={{song.pdf_path.pdf_path.url}}>pdf</a></td>
            </tr>

        {{/each}}
    </table>

</div>


